<% content_for :page_title do %>Admin Reports<% end %>
<div class="application container">
  <div class="row padded-row">
    <div class="col-md-9">
      <h1>Admin Reports</h1>
    </div>
  </div>

  <div class="row">
    <ul>
      <li><%= link_to "AMR Report", reports_amr_data_index_path %></li>
      <li><%= link_to "Data Feeds summary", data_feeds_path %></li>
      <% DataFeeds::WeatherUnderground.all.each do |data_feed| %>
        <li><%= link_to "#{data_feed.title} - temperature", reports_data_feed_show_path(data_feed, :temperature) %></li>
        <li><%= link_to "#{data_feed.title} - solar irradiation", reports_data_feed_show_path(data_feed, :solar_irradiation) %></li>
      <% end %>
      <% DataFeeds::SolarPvTuos.all.each do |data_feed| %>
        <li><%= link_to  data_feed.title, reports_data_feed_show_path(data_feed, :solar_pv) %></li>
      <% end %>
    </ul>
  </div>
</div>
