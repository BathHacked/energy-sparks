wb = xlsx_package.workbook

wb.add_worksheet(name: "Data Feeds") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row %w(ReadingDate OneDayKWHTotal SubstitutionDate Status)

  # each user is a row on your spreadsheet
  @amr_validated_meter_readings.each do |reading|
    sheet.add_row [reading.reading_date, reading.one_day_kwh, reading.status, reading.substitute_date, *reading.kwh_data_x48]
  end
end
