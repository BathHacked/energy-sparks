wb = xlsx_package.workbook

date_style = wb.styles.add_style(:format_code => 'yyyy-mm-dd hh:mm:ss')

wb.add_worksheet(name: "Data Feeds") do |sheet|
  # this is the head row of your spreadsheet
  sheet.add_row %w(At Value)

  # each user is a row on your spreadsheet
  @readings.each do |reading|
    sheet.add_row [reading.at, reading.value]
  end
  sheet.cols[0].each { |c| c.style = date_style }

end