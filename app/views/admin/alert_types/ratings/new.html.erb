<%= render 'admin/alert_types/nav', alert_type: @alert_type %>

<h2>New content</h2>

<%= simple_form_for @rating, url: admin_alert_type_ratings_path(@alert_type) do |form| %>
  <%= render 'form', form: form, content: @content, template_variables: @template_variables %>
  <%= form.button :submit, 'Create content' %>
<% end %>

